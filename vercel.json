{
  "version": 2,
  "builds": [
    {
      "src": "apps/frontend/package.json",
      "use": "@vercel/next"
    }
  ],
  "installCommand": "npm install --legacy-peer-deps",
  "buildCommand": "npm --workspace=@magajico/shared run build && npm --workspace=@magajico/frontend run build",
  "devCommand": "npm --workspace=@magajico/frontend run dev",
  "framework": null,
  "outputDirectory": "apps/frontend/.next",
  "ignoreCommand": "git diff --quiet HEAD^ HEAD ./apps/frontend ./packages/shared"
}