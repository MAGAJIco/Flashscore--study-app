modules = ["nodejs-20", "python-3.11"]
run = "cd apps/frontend && npm run dev"

[nix]
channel = "stable-25_05"
packages = ["miktex", "nix-output-monitor", "try", "pnpm"]

[deployment]
run = ["bash", "-c", "cd apps/frontend && pnpm start"]
build = ["bash", "-c", "pnpm install && cd apps/frontend && pnpm build"]
deploymentTarget = "autoscale"

[workflows]
runButton = "Frontend Fixed Port"

[[workflows.workflow]]
name = "Start Sports API"
author = 46296136
mode = "sequential"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npx ts-node --esm Sports-api.ts"

[[workflows.workflow]]
name = "Full Stack Next.js"
author = 46296136
mode = "parallel"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npm install tsx --save-dev && npx tsx server.ts"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "npm run dev"

[[workflows.workflow]]
name = "Full Stack Monorepo"
author = 46296136
mode = "parallel"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "cd apps/backend && npm run dev"

[[workflows.workflow.tasks]]
task = "shell.exec"
args = "cd apps/frontend && npm run dev"

